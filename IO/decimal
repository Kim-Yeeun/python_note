
#----------------------------------------------------
# 10진수를 16진수로 출력

a = int(input()) # 255
print('%x' % a) # ff

a = int(input()) # 255
print('%X' % a) # FF

#----------------------------------------------------
# 16진수를 8진수로 출력

a = int(input(), 16)
print('%o' % a)

#----------------------------------------------------
# 문자를 10진수로 출력

s = ord(input()) # A
print(s) # 65

#----------------------------------------------------
# 10진수를 문자로 출력

n = int(input()) # 65
print(chr(n)) # A

#----------------------------------------------------
# 다음 문자 출력

s = ord(input())
print(chr(s + 1))

#----------------------------------------------------
# 16진수 구구단 출력

s = int(input(), 16)
for i in range(1, 16):
  print('%X'%s, '*%X'%i, '=%X'%(s*i), sep='')
  

