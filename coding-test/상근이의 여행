
import sys
input = sys.stdin.readline

def dfs(v, cnt):
  visited[v] = True
  for i in graph[v]:
    if not visited[i]:
      cnt += 1
      cnt = dfs(i, cnt)
  return cnt

result = []
for _ in range(int(input())):
  n, m = map(int, input().split())
  
  graph = [[] for _ in range(n+1)]
  for _ in range(m):
    a, b = map(int, input().split())
    graph[a].append(b)
    graph[b].append(a)
    
  visited = [False] * (n+1)

  result.append(dfs(1, 0))

for i in result:
  print(i)
  
  
