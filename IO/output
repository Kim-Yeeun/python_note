
#----------------------------------------------------------------
# 줄바꿈 없이 출력하기

a = 1
b = 2

print(a, b) # 1 2 
print(7, end = " ")
print(8, end = " ") # 7 8

#----------------------------------------------------------------
# f-string

answer = 7
print(f"정답은 {answer}입니다.") # 정답은 7입니다.

#----------------------------------------------------------------
# format(소수점 반올림)

f = float(input()) # 3.141592
print(format(f, '.2f')) # 3.14



